/* pages/userlogin/userlogin.wxss */
.main {
    position: absolute;
    width: 100%;
    height: 100%;
    background: url("https://z3.ax1x.com/2021/11/09/ItbUi9.png") no-repeat;
}

.login {
    display: flex;
    margin-left: 68rpx;
    margin-top: 20rpx;
    width: 300rpx;
    height: 100rpx;
    font-size: 60rpx;
    color: #333333;
}

.box-login {
    width: 90%;
    height: 43%;
    margin: 0 auto;
    /* text-align: center; */
    border: 3px solid rgb(211, 208, 208);
    /* box-shadow: 4px 4px 4px #bdbcbc; */
    background: #fff;
    border-radius: 10px;
    margin-top: 50%;
}

.input {
    margin: 40rpx auto;
    margin-bottom: 0;
    text-align: center;
}


.tips {
    line-height: 100rpx;
    font-size: 0.2em;
    text-align: center;
    color: #999;
}

.btn {
    margin-bottom: auto;
}

.butt {
    color: rgb(250, 249, 249);
    width: 200rpx;
    margin: auto;
    text-align: center;
    margin-top: 330rpx;
}

.bottom {
    width: 80%;
}

.admin {
    position: absolute;
    top: 5%;
    right: 5%;
    color: #333333;
}

.divLine {
    background: #E0E3DA;
    width: 60%;
    margin-left: 200rpx;
    height: 5rpx;
}